<!DOCTYPE html>
<html>
<head>
  <title><%= category %></title>
</head>
<body>
    <%- include('./partials/nav') %>
  <h1><%= category %></h1>
  <% menuItems.forEach(function(item) { %>
    <h2><%= item.name %></h2>
    <p>Price: <%= item.price %></p>
    <p>Rating: <%= item.rating %></p>
    <p>Details: <%= item.details %></p>
  <% }); %>
</body>
</html>